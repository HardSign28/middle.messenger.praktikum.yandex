<div class="page page-navigate bg-gradient">
	<div class="container w-100">
		<div class="card">
			<h1 class="card__title text-center">
				Навигация
			</h1>
			<nav>
				<ul class="navigate-list">
					{{#each pages}}
						{{#unless (eq @key "nav")}}
							<li class="navigate-list__item">
								<a
									class="navigate-list__item-link"
									href="#"
									page="{{@key}}"
								>
									{{this.name}}
								</a>
							</li>
						{{/unless}}
					{{/each}}
				</ul>
			</nav>
		</div>
	</div>
</div>