<div class="dialog-container">
	<div class="dialog">
		<div class="dialog__close"></div>
		<div class="dialog__header">
			<h2 class="dialog__title">{{ title }}</h2>
		</div>
		<div class="dialog__body">{{> @partial-block }}</div>
		<div class="dialog__footer">
			{{#if labelCancel }}
				{{> Button label=labelCancel type="outline-primary" }}
			{{/if}}
			{{> Button label=labelOk type="primary"}}
		</div>
	</div>
</div>
